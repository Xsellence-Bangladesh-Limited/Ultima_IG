<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- ultima.layout, generated on August 13, 2024 -->
    <!-- Tree view of ultima.layout -->
    <record model="ir.ui.view" id="ultima_layout_tree_id">
        <field name="name">UltimaLayout</field>
        <field name="model">ultima.layout</field>
        <field name="arch" type="xml">
            <tree>
                <field name="address"/>
                <field name="contact_no"/>
                <field name="email"/>
            </tree>
        </field>
    </record>

    <!-- Form view of ultima.layout -->
    <record model="ir.ui.view" id="ultima_layout_form_id">
        <field name="name">UltimaLayout</field>
        <field name="model">ultima.layout</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="footer_desc"/>
                            <field name="note"/>
                            <field name="address"/>
                            <field name="contact_no"/>
                            <field name="whatsapp"/>
                            <field name="email"/>
                            <field name="quick_link_ids"/>
                            <field name="social_ids"/>
                        </group>
                        <group>
                            <field name="logo_1920" widget="image" class="oe_avatar"/>
                            <field name="footer_logo_1920" widget="image" class="oe_avatar"/>
                            <field name="map_1920" widget="image" class="oe_avatar"/>
                            <field name="map_url"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Action view of ultima.layout -->
    <record model="ir.actions.act_window" id="ultima_layout_action_id">
        <field name="name">UltimaLayout</field>
        <field name="res_model">ultima.layout</field>
        <field name="view_mode">tree,form</field>
    </record>

