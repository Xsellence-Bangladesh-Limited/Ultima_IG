    <!-- ultima.home, generated on August 14, 2024 -->
    <!-- Tree view of ultima.home -->
    <record model="ir.ui.view" id="ultima_home_tree_id">
        <field name="name">UltimaHome</field>
        <field name="model">ultima.home</field>
        <field name="arch" type="xml">
            <tree>
                <field name="bsp_section"/>
                <field name="bsp_title"/>
                <field name="bsp_desc"/>
                <field name="dis_section"/>
                <field name="dis_title"/>
            </tree>
        </field>
    </record>

    <!-- Form view of ultima.home -->
    <record model="ir.ui.view" id="ultima_home_form_id">
        <field name="name">UltimaHome</field>
        <field name="model">ultima.home</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <separator string="Best Selling Products"/>
                        <field name="bsp_section"/>
                        <field name="bsp_title"/>
                        <field name="bsp_desc"/>

                        <separator string="Discover"/>
                        <field name="dis_section"/>
                        <field name="dis_title"/>
                        <field name="dis_desc"/>
                        <field name="dis_line_ids"/>

                        <separator string="Types"/>
                        <field name="type_section"/>
                        <field name="type_title"/>
                        <field name="type_ids"/>

                        <separator string="Comparison"/>
                        <field name="com_section"/>
                        <field name="com_title"/>
                        <field name="com_ids"/>

                        <separator string="Clients"/>
                        <field name="client_title"/>
                        <field name="client_img_1920" widget="image" class="oe_avatar"/>

                        <separator string="FAQ"/>
                        <field name="faq_section"/>
                        <field name="faq_title"/>
                        <field name="faq_ids"/>

                    </group>
                </sheet>
            </form>
        </field>
    </record>

