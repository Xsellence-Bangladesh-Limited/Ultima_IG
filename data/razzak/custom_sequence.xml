<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <data noupdate="1">
        <record id="ultima_product_order_seq" model="ir.sequence">
            <field name="name">Ultima product order sequence</field>
            <field name="code">ultima.product.order.seq</field>
            <field name="prefix">OR</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
        </record>

        <record id="ultima_expert_message_seq" model="ir.sequence">
            <field name="name">Ultima expert message sequence</field>
            <field name="code">ultima.expert.message.seq</field>
            <field name="prefix">EXPM</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
        </record>

        <record id="ultima_payment_seq" model="ir.sequence">
            <field name="name">Ultima payment sequence</field>
            <field name="code">ultima.payment.seq</field>
            <field name="prefix">PAYMENT</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
        </record>
    </data>
</odoo>